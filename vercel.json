{
    "version": 2,
    "builds": [
        {
            "src": "backend/src/index.ts",
            "use": "@vercel/node",
            "config": { "includeFiles": ["backend/dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/dist/index.js"
        }
    ]
}
